{
  "name": "yuits",
  "version": "1.0.0",
  "description": "YuiBot TypeScript version",
  "main": "yui/yui.ts",
  "scripts": {
    "prebuild-win": "IF EXIST .\\dist rmdir /Q /S .\\dist && mkdir dist",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run prebuild && export NODE_ENV=production && tsc -p tsconfig.json && node dist/yui.js",
    "dev": "npm run prebuild && export NODE_ENV=development && tsc -p tsconfig.json && node dist/yui.js",
    "build": "npm run prebuild && export NODE_ENV=production && tsc -p tsconfig.json",
    "start:dev": "export NODE_ENV=development && tsc-watch -p tsconfig.build.json --onSuccess \"node dist/yui.js\"",
    "start:prod": "export NODE_ENV=production tsc-watch -p tsconfig.build.json --onSuccess \"node dist/yui.js\"",
    "start:win": "npm run prebuild-win && set NODE_ENV=development && tsc -p tsconfig.json && node dist/yui.js",
    "cpyfiles": "copyfiles --up 2 --all yui/assets/**/* dist/assets",
    "pm2-service": "export NODE_ENV=production && pm2 start dist/yui.js"
  },
  "author": "Polaris",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.1.0",
    "@discordjs/uws": "^11.149.1",
    "axios": "^0.19.2",
    "bufferutil": "^4.0.1",
    "colors": "^1.4.0",
    "copyfiles": "^2.2.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "erlpack": "github:discordapp/erlpack",
    "ffmpeg-static": "^4.2.1",
    "fs": "^0.0.1-security",
    "google-auth-library": "^5.10.1",
    "googleapis": "^48.0.0",
    "handlebars": "^4.7.6",
    "install": "^0.13.0",
    "libsodium-wrappers": "^0.7.6",
    "lodash": "^4.17.15",
    "log4js": "^6.1.2",
    "m3u8stream": "^0.7.1",
    "miniget": "^1.7.0",
    "module-alias": "^2.2.2",
    "npm": "^6.14.5",
    "opusscript": "^0.0.7",
    "path": "^0.12.7",
    "pm2": "^4.4.0",
    "prism-media": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "tslib": "^1.11.2",
    "utf-8-validate": "^5.0.2",
    "ytdl-core": "^2.1.0",
    "ytdl-core-discord": "^1.2.0",
    "zlib-sync": "^0.1.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.150",
    "@types/node": "^12.12.38",
    "node-gyp-build": "^4.2.2",
    "node-pre-gyp": "^0.14.0",
    "patch-package": "^6.2.2",
    "tsc": "^1.20150623.0",
    "tsc-watch": "^4.2.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@": "dist",
    "config-service": "dist/config-service"
  }
}
